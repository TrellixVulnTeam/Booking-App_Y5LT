import { EventEmitter, OnInit } from '@angular/core';
import { NgxTimeDurationPickerComponent } from './ngx-time-duration-picker.component';
export declare class NgxTimeDurationPickerUnitComponent implements OnInit {
    timeDurationPicker: NgxTimeDurationPickerComponent;
    name: string;
    label: string;
    max: number;
    min: number;
    step: number;
    inputDisabled: boolean;
    OnChange: EventEmitter<number>;
    value: number;
    constructor(timeDurationPicker: NgxTimeDurationPickerComponent);
    ngOnInit(): void;
    increaseValue(): void;
    decreaseValue(): void;
    checkValue(event: any): void;
    updateValue(): void;
    getValue(): number;
}
